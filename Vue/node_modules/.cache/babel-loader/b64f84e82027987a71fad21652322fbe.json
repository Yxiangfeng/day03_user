sContent":["<template>\r\n\r\n  <el-card class=\"container\" :id=\"doctorInfo.doctorId\">\r\n    <div style=\"font-size: 18px;\">医生 <i>{{doctorInfo.doctorName}}</i> 详细资料 <el-button v-if=\"user.role===1\" style=\"margin-left:20px;\" type=\"Info\" plain size=\"mini\" @click=\"handleEdit(doctorInfo)\">选择</el-button></div>\r\n    <hr/>\r\n    <div class=\"box-card\" >\r\n      <img :src=\"doctorInfo.doctorPic\">\r\n      <div class=\"caption\">\r\n        <h1>{{doctorInfo.doctorName}}</h1>\r\n        <h2>{{doctorInfo.doctorAge}}岁</h2>\r\n        <h2>{{doctorInfo.doctorPosition}}</h2>\r\n      </div>\r\n    </div>\r\n    <hr/>\r\n    <h3>擅长领域：<span>{{doctorInfo.doctorDes}}</span></h3>\r\n    <hr/>\r\n    <div v-if=\"user.role!==2\" style=\"margin:10px 0 0 400px;\"><el-button type=\"primary\" plain size=\"normal\" @click=\"handleSelect\">选择该医生</el-button></div>\r\n  </el-card>\r\n\r\n  <el-dialog title=\"更新医生信息\" v-model=\"dialogVisible\" width=\"30%\">\r\n    <el-form :model=\"form\" label-width=\"120px\">\r\n      <el-form-item label=\"姓名\">\r\n        <el-input v-model=\"form.doctorName\" style=\"width:80%\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"年龄\">\r\n        <el-input v-model=\"form.doctorAge\" style=\"width:80%\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"职位\">\r\n        <el-input v-model=\"form.doctorPosition\" style=\"width:80%\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"描述\">\r\n        <el-input v-model=\"form.doctorDes\" style=\"width:80%\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"图片\">\r\n        <el-upload ref=\"upload\" class=\"upload-demo\" action=\"http://localhost:8887/files/upload\" :on-success=\"filesUploadSuccess\">\r\n          <el-button type=\"primary\">点击上传</el-button>\r\n        </el-upload>\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n          <span class=\"dialog-footer\">\r\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"saveData\">确 定</el-button>\r\n          </span>\r\n    </template>\r\n  </el-dialog>\r\n\r\n  <el-dialog title=\"提交自测生理参数\" v-model=\"dialogVisible2\" width=\"30%\">\r\n    <el-upload ref=\"upload\" class=\"upload-demo\" action=\"http://localhost:8887/files/uploadTxt\" :on-success=\"filesUploadSuccess2\">\r\n      <el-button type=\"primary\">点击上传文件</el-button>\r\n    </el-upload>\r\n    <template #footer>\r\n          <span class=\"dialog-footer\">\r\n            <el-button @click=\"dialogVisible2 = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"selectDoctor\">确 定</el-button>\r\n          </span>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"seeDoctorInfo\",\r\n  data() {\r\n    return {\r\n      doctor: {},\r\n      user:{},\r\n      form:{},\r\n      doctorInfo:{},\r\n      dialogVisible:false,\r\n      dialogVisible2:false,\r\n    }\r\n  },\r\n  created() {\r\n    let str = sessionStorage.getItem(\"doctor\") || \"{}\"\r\n    this.doctor = JSON.parse(str)\r\n    let str1 = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(str1)\r\n    this.load()\r\n  },\r\n  methods: {\r\n    filesUploadSuccess(res){\r\n      console.log(res)\r\n      this.form.doctorPic = res.data\r\n    },\r\n    filesUploadSuccess2(res){\r\n      console.log(res)\r\n\r\n    },\r\n    load(){\r\n      request.get(\"/api/allUser/findOneDoctor\",{\r\n        params:{\r\n          doctorId:this.doctor.doctorId\r\n        }\r\n      }).then(res =>{\r\n        console.log(res)\r\n        this.doctorInfo = res.data\r\n      })\r\n    },\r\n    handleEdit(row){\r\n      //\r\n      this.form = row\r\n      this.dialogVisible = true\r\n    },\r\n\r\n    handleSelect(){\r\n      this.dialogVisible2 = true\r\n    },\r\n\r\n    selectDoctor(){\r\n      request.get(\"/api/allUser/selectDoctor\",{\r\n        params:{\r\n          userId:this.user.id,\r\n          doctorId:this.doctor.doctorId\r\n        }\r\n      }).then(res=>{\r\n        if(res.code === 0){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"已成功将文件发送给医生\"\r\n          })}else{\r\n          this.$message({\r\n            type:\"error\",\r\n            message:res.msg\r\n          })\r\n        }\r\n      })\r\n      this.dialogVisible2 = false\r\n    },\r\n\r\n    saveData(){\r\n      if(this.form.doctorId){\r\n        request.put(\"/api/allUser/updateDoctorData\",this.form).then(res =>{\r\n          console.log(res)\r\n          if(res.code === 0){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"修改成功\"\r\n            })}else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n        })\r\n      }else {\r\n        request.post(\"/api/allUser/saveDoctorData\", this.form).then(res => {\r\n          console.log(res)\r\n          if(res.code === 0){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"添加成功\"\r\n            })}else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n        })\r\n      }\r\n      this.dialogVisible = false\r\n      this.load()\r\n    },\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .container{\r\n    width:1250px;\r\n    height:1000px;\r\n  }\r\n  .box-card{\r\n    display:flex;\r\n    margin-top: 10px;\r\n    align-items: center;\r\n  }\r\n  .box-card > img{\r\n    margin-left: 50px;\r\n    width:240px;\r\n    height:240px;\r\n  }\r\n  .caption{\r\n    margin-left: 100px;\r\n    height:240px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n  }\r\n  .caption > h1,.caption > h2{\r\n    width:200px;\r\n    font-weight: bold;\r\n    margin-bottom: 30px;\r\n  }\r\n\r\n</style>\r\n"],"sourceRoot":""}]}