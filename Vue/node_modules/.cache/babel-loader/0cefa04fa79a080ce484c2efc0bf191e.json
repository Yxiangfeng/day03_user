gBAAA,KAAiB,EAAjB;AAAA,2BAAA;AAAA,iBAAsC;AAApB,gBAAA,QAAQ,EAAR;AAAoB,eAAxE,E,IAAA,E;;AAAA,gB,cAAA,CAAwE,CAAxE;AAAA,a,CADsB;;;;AAAA,WAAxB,CAEe,EACf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACtB;AAAA,qBAAyE,CAAzE,YAAA,CAAyE,mBAAzE,EAAyE;4BAAtD,KAAA,CAAA,IAAA,CAAK,SAAiD;;yBAAtD,KAAA,CAAA,IAAA,CAAK,SAAL,GAAc,M;kBAAwC;AAAtC,gBAAA,KAAiB,EAAjB;AAAA,2BAAA;AAAA,iBAAsC;AAApB,gBAAA,QAAQ,EAAR;AAAoB,eAAzE,E,IAAA,E;;AAAA,gB,cAAA,CAAyE,CAAzE;AAAA,a,CADsB;;;;AAAA,WAAxB,CADe,EAIvB,mBAAA,CAAA,qCAAA,CAJuB,EAKvB,mBAAA,CAAA,wDAAA,CALuB,EAMvB,mBAAA,CAAA,yBAAA,CANuB,CAFf;AAAA,S;;;;OADF,E;;AAAA,Q,SAAA,CAUU,CAVV;AAAA,K;;;;GADF,E;;AAAA,I,cAAA,CAmBI,CA/FN,C","sourcesContent":["<template>\r\n  <div class=\"home\" style=\"padding: 10px\">\r\n    <!--    <img alt=\"Vue logo\" src=\"../assets/logo.png\">-->\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input v-model=\"search\" placeholder=\"please input\" style=\"width:20%\" clearable></el-input>\r\n      <el-button type=\"primary\" style=\"margin-left:5px\" @click=\"load\">查询</el-button>\r\n    </div>\r\n    <el-table\r\n        :data=\"tableData\"\r\n        border\r\n        align=\"center\"\r\n        style=\"width: 100%\">\r\n      <el-table-column\r\n          prop=\"dishName\"\r\n          label=\"菜名\">\r\n      </el-table-column>\r\n      <el-table-column\r\n          prop=\"dishPrice\"\r\n          label=\"价格\">\r\n      </el-table-column>\r\n      <el-table-column\r\n          label=\"图片\">\r\n        <template #default=\"scope\">\r\n          <el-image\r\n              style=\"width: 120px; height: 120px\"\r\n              fit=\"cover\"\r\n              :src=\"scope.row.dishPicPath\"\r\n              :preview-src-list=\"[scope.row.dishPicPath]\">\r\n          </el-image>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\"\r\n      width=\"100px\">\r\n        <template #default=\"scope\">\r\n          <el-button type=\"primary\" size=\"normal\" @click=\"handleEdit(scope.row)\">选择</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n\r\n      <div  v-for=\"(good, index) in tableData\" :key=\"good.dishID\">\r\n      <span>{{good.dishName}}</span>\r\n      <span>￥{{good.dishPrice}}</span>\r\n<!--      <template #default=\"scope\">-->\r\n<!--          <el-image-->\r\n<!--              style=\"width: 120px; height: 120px\"-->\r\n<!--              fit=\"cover\"-->\r\n<!--              :src=\"good.dishPicPath\"-->\r\n<!--              :preview-src-list=\"[good.dishPicPath]\">-->\r\n<!--          </el-image>-->\r\n<!--        </template>-->\r\n        <div style=\"padding: 20px\">\r\n          <img :src=\"good.dishPicPath\" alt=\"...\">\r\n          <div class=\"caption\">\r\n            <h3>Thumbnail label</h3>\r\n            <p>...</p>\r\n            <p><a href=\"#\" class=\"btn btn-primary\" role=\"button\">Button</a> <a href=\"#\" class=\"btn btn-default\" role=\"button\">Button</a></p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[3, 6, 9]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"点菜\" v-model=\"dialogVisible\" width=\"30%\">\r\n      <el-form :model=\"form\" label-width=\"120px\">\r\n        <el-form-item label=\"菜名\">\r\n          <el-input v-model=\"form.dishName\" style=\"width:80%\" readonly></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"价格\">\r\n          <el-input v-model=\"form.dishPrice\" style=\"width:80%\" readonly></el-input>\r\n        </el-form-item>\r\n<!--        <el-form-item label=\"价格\">-->\r\n<!--          <el-input hidden>{{manager.name}}</el-input>-->\r\n<!--        </el-form-item>-->\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"orderData\">确 定</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport manager from \"@/views/manager\";\r\nexport default {\r\n  name: \"IndexOrder\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      form: {},\r\n      dialogVisible: false,\r\n      search: '',\r\n      currentPage: 1,\r\n      pageSize: 3,\r\n      total: 0,\r\n      tableData: [],\r\n      user:{}\r\n\r\n    }\r\n  },\r\n  created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(str)\r\n    this.load();\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get(\"/api/dish/findPage\", {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n        }\r\n      }).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n        this.currentPage = res.data.current\r\n        this.pageSize = res.data.size\r\n      })\r\n    },\r\n\r\n    orderData() {\r\n        request.post(\"/api/dishorder/saveID\", this.user)\r\n        console.log(this.user)\r\n        request.post(\"/api/dishorder/saveData\", this.form).then(res => {\r\n          console.log(res)\r\n          if (res.code == 0) {\r\n            this.$message({\r\n              type: \"success\",\r\n              message: \"添加成功\"\r\n            })\r\n          } else {\r\n            this.$message({\r\n              type: \"error\",\r\n              message: res.msg\r\n            })\r\n          }\r\n        })\r\n      this.dialogVisible = false\r\n      this.load()\r\n    },\r\n\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogVisible = true\r\n\r\n    },\r\n\r\n    handleSizeChange(pageSize) {\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n\r\n    handleCurrentChange(pageNum) {\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>"],"sourceRoot":""}]}