essionStorage.setItem(\"user\", JSON.stringify(this.form))\r\n          let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n          this.form = JSON.parse(str)\r\n          this.$store.commit(\"update\",this.form)\r\n          this.userPhoto = this.$store.state.userPhoto\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n      this.dialogVisible2 = false\r\n    },\r\n    update() {\r\n      request.put(\"/api/allUser/updateDataManager\", this.form).then(res => {\r\n        console.log(res)\r\n        if (res.code === 0) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"更新成功\"\r\n          })\r\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form))\r\n          let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n          this.form = JSON.parse(str)\r\n          this.$store.commit(\"update\",this.form)\r\n          this.userPhoto = this.$store.state.userPhoto\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n      this.dialogVisible2 = false\r\n    },\r\n\r\n    load(){\r\n      request.post(\"/api/allUser/showOneInfo\",this.form).then(res =>{\r\n        console.log(res)\r\n        this.form = res.data\r\n      })\r\n      if(this.form.role === 3){\r\n        this.loadCp()\r\n      }\r\n    },\r\n\r\n    loadCp(){\r\n      request.get(\"/api/cp/cpByPatientId1\",{\r\n        params:{\r\n          id:this.form.id\r\n        }}).then(res =>{\r\n        console.log(res)\r\n        this.cpList = res.data\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container{\r\n  width:920px;\r\n  min-height:1000px;\r\n  background-color: #FEFDFD\r\n}\r\n\r\n.caption > h1,.caption > h2{\r\n  width:200px;\r\n  font-weight: bold;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.cell-item {\r\n  di