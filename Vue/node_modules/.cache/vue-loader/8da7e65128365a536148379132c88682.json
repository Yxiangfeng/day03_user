w: hidden\">\r\n  <div style=\"width:400px;margin:150px auto;\">\r\n    <div style=\"color: #cccccc;font-size: 30px;text-align: center;padding: 30px 0;\">\r\n        欢迎登录\r\n    </div>\r\n    <el-form ref=\"form\" :rules=\"rules\" :model=\"form\" size=\"normal\">\r\n      <el-form-item prop=\"name\">\r\n        <el-input prefix-icon=\"el-icon-user-solid\" v-model=\"form.name\" ></el-input>\r\n      </el-form-item>\r\n      <el-form-item prop=\"password\">\r\n        <el-input prefix-icon=\"el-icon-lock\" v-model=\"form.password\" show-password></el-input>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button style=\"width:40%;\" type=\"primary\" @click=\"login\">登录</el-button>\r\n        <el-button style=\"width:40%;margin-left: 80px\" type=\"primary\" @click=\"$router.push('/register')\">注册</el-button>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button style=\"width:100%;\" type=\"primary\" @click=\"managerLogin\">管理员登录</el-button>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button style=\"width:100%;\" type=\"primary\" @click=\"workerLogin\">员工登录</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  data(){\r\n    return {\r\n      form:{},\r\n      rules: {\r\n       name: [\r\n          { required: true, message: '请输入用户名', trigger: 'blur' },\r\n          // { min: 3, max: 5, message: '长度在 3 到 5 个字符', trigger: 'blur' }\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'blur' },\r\n          // { min: 3, max: 5, message: '长度在 3 到 5 个字符', trigger: 'blur' }\r\n        ]\r\n      }\r\n    };\r\n  },\r\n\r\n  created(){\r\n    sessionStorage.removeItem(\"manager\"&&\"user\"&&\"worker\")\r\n  },\r\n  methods:{\r\n    login(){\r\n      this.$refs['form'].validate((valid) => {\r\n        if (valid) {\r\n          request.post(\"/api/user/login\",this.form).then(res=>{\r\n            if(res.code == 0){\r\n              this.$message({\r\n                type:\"success\",\r\n                message:\"登录成功！\"\r\n              })\r\n              sessionStorage.setItem(\"user\", JSON.stringify(res.data))  // 缓存用户信息\r\n              this.$router.push(\"/index\") //页面跳转\r\n            }else {\r\n              this.$message({\r\n                type:\"error\",\r\n                message:res.msg\r\n              })\r\n            }\r\n          })\r\n        }\r\n      });\r\n    },\r\n    managerLogin(){\r\n      this.$refs['form'].validate((valid) => {\r\n        if (valid) {\r\n          request.post(\"/api/manager/login\",this.form).then(res=>{\r\n            if(res.code == 0){\r\n              this.$message({\r\n                type:\"success\",\r\n                message:\"登录成功！\"\r\n              })\r\n              sessionStorage.setItem(\"manager\", JSON.stringify(res.data))  // 缓存用户信息\r\n              this.$router.push(\"/\") //页面跳转\r\n            }else {\r\n              this.$message({\r\n                type:\"error\",\r\n                message:res.msg\r\n              })\r\n            }\r\n          })\r\n        }\r\n      });\r\n    },\r\n    workerLogin(){\r\n      this.$refs['form'].validate((valid) => {\r\n        if (valid) {\r\n          request.post(\"/api/worker/login\",this.form).then(res=>{\r\n            if(res.code == 0){\r\n              this.$message({\r\n                type:\"success\",\r\n                message:\"登录成功！\"\r\n              })\r\n              sessionStorage.setItem(\"worker\", JSON.stringify(res.data))  // 缓存用户信息\r\n              this.$router.push(\"/index\") //页面跳转\r\n            }else {\r\n              this.$message({\r\n                type:\"error\",\r\n                message:res.msg\r\n              })\r\n            }\r\n          })\r\n        }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}