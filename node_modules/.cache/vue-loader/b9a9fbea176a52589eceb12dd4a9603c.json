rm: {},\r\n      dialogVisible2:false,\r\n      cp: {},\r\n      desc:\"\"\r\n    }\r\n  },\r\n\r\n  created() {\r\n    let str = sessionStorage.getItem(\"patient\") || \"{}\"\r\n    this.form = JSON.parse(str)\r\n    this.$nextTick(()=>{\r\n      // if(!editor) {\r\n      //   editor = new E(\"#richText\")\r\n      //   editor.config.uploadImgServer = 'http://localhost:8887/files/uploadImg'\r\n      //   editor.config.uploadFileName = 'file'\r\n      //   editor.create()\r\n      // }\r\n      // editor.txt.html('')  // 清除内容\r\n      this.draw()\r\n    })\r\n    this.loadCp()\r\n  },\r\n\r\n  methods: {\r\n    draw(){\r\n      let hl = document.getElementsByClassName(\"st0\")[0]\r\n      hl.style.fill = \"red\"\r\n    },\r\n    loadCp(){\r\n      request.get(\"/api/cp/cpList\",{\r\n       params:{\r\n         cpId: this.form.cpId\r\n       }\r\n      }).then(res =>{\r\n        this.cp = res.data\r\n        console.log(this.cp.paraHL)\r\n      })\r\n    },\r\n\r\n    saveData(){\r\n      this.cp.caseDesc = this.desc\r\n      request.post(\"/api/cp/updateCaseDesc\", this.cp).then(res => {\r\n        if(res.code === 0){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"保存成功\"\r\n          })\r\n          this. loadCp()\r\n        }else{\r\n          this.$message({\r\n            type:\"error\",\r\n            message:res.msg\r\n          })\r\n        }\r\n      })\r\n      this.desc = ''\r\n    },\r\n\r\n    handleSelect(){\r\n      this.dialogVisible2 = true\r\n      this.$refs['upload'].clearFiles()\r\n    },\r\n\r\n    cancel(){\r\n      let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n      this.form = JSON.parse(str)\r\n      this.dialogVisible2 = false\r\n    },\r\n    update() {\r\n      request.put(\"/api/allUser/updateData\", this.form).then(res => {\r\n        console.log(res)\r\n        if (res.code === 0) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"更新成功\"\r\n          })\r\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form))\r\n          let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n          this.form = JSON.parse(str)\r\n          this.$store.commit(\"update\",this.form)\r\n          this.userPhoto = this.$store.state.userPhoto\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n      this.dialogVisible2 = false\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container{\r\n  width:920px;\r\n  min-height:1000px;\r\n  background-color: #FEFDFD\r\n}\r\n.box-card{\r\n  display:flex;\r\n  margin-top: 10px;\r\n  align-items: center;\r\n}\r\n.box-card > img{\r\n  margin-left: 50px;\r\n  width:240px;\r\n  height:240px;\r\n}\r\n.caption{\r\n  margin-left: 100px;\r\n  height:240px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: flex-start;\r\n  align-items: center;\r\n}\r\n.caption > h1,.caption > h2{\r\n  width:200px;\r\n  font-weight: bold;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.st0{fill:#801381;}\r\n.st1{fill:#836CAF;}\r\n.st2{fill:#781D75;}\r\n.st3{fill:#688A32;}\r\n.st4{fill:#3E2965;}\r\n.st5{fill:#3B83C5;}\r\n.st6{fill:#F29A71;}\r\n.st7{fill:#E61C16;}\r\n.st8{fill:#5E9999;}\r\n.st9{fill:#95C436;}\r\n.st10{fill:#FFFFFF;}\r\n.st11{font-family:'ArialMT';}\r\n.st12{font-size:7.8364px;}\r\n.st13{font-size:6.0961px;}\r\n\r\n</style>\r\n"]}]}