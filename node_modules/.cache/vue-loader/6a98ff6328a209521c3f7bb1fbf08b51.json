tr)\r\n    this.load();\r\n  },\r\n  methods: {\r\n    Flag(){\r\n      //alert(\"123\")\r\n      request.post(\"/api/dishorder/saveID\", this.user) //传递用户ID\r\n      request.post(\"/api/dishorder/saveFlag\").then(res => {//生成唯一的订单号，并加到点菜表里面\r\n        console.log(res)\r\n        if (res.code == 0) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"请开始点餐\"\r\n          })\r\n        }\r\n      })\r\n\r\n    },\r\n    zc(){request.get(\"/api/dish/findDish\", {\r\n      params: {\r\n        pageNum: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: '0'\r\n      }\r\n      }).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n        this.currentPage = res.data.current\r\n        this.pageSize = res.data.size\r\n      })\r\n    },\r\n    xc(){request.get(\"/api/dish/findDish\", {\r\n      params: {\r\n        pageNum: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: '1'\r\n      }\r\n    }).then(res => {\r\n      console.log(res)\r\n      this.tableData = res.data.records\r\n      this.total = res.data.total\r\n      this.currentPage = res.data.current\r\n      this.pageSize = res.data.size\r\n    })\r\n    },\r\n    soup(){request.get(\"/api/dish/findDish\", {\r\n      params: {\r\n        pageNum: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: '2'\r\n      }\r\n    }).then(res => {\r\n      console.log(res)\r\n      this.tableData = res.data.records\r\n      this.total = res.data.total\r\n      this.currentPage = res.data.current\r\n      this.pageSize = res.data.size\r\n    })\r\n    },\r\n    dessert(){request.get(\"/api/dish/findDish\", {\r\n      params: {\r\n        pageNum: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: '3'\r\n      }\r\n    }).then(res => {\r\n      console.log(res)\r\n      this.tableData = res.data.records\r\n      this.total = res.data.total\r\n      this.currentPage = res.data.current\r\n      this.pageSize = res.data.size\r\n    })\r\n    },\r\n    drink(){request.get(\"/api/dish/findDish\", {\r\n      params: {\r\n        pageNum: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: '4'\r\n      }\r\n    }).then(res => {\r\n      console.log(res)\r\n      this.tableData = res.data.records\r\n      this.total = res.data.total\r\n      this.currentPage = res.data.current\r\n      this.pageSize = res.data.size\r\n    })\r\n    },\r\n    load() {\r\n      request.get(\"/api/dish/findPage\", {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n        }\r\n      }).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n        this.currentPage = res.data.current\r\n        this.pageSize = res.data.size\r\n      })\r\n    },\r\n\r\n    dishData() {\r\n        request.post(\"/api/dishorder/saveData\", this.form).then(res => {\r\n          console.log(res)\r\n          if (res.code == 0) {\r\n            this.$message({\r\n              type: \"success\",\r\n              message: \"添加成功\"\r\n            })\r\n          } else {\r\n            this.$message({\r\n              type: \"error\",\r\n              message: res.msg\r\n            })\r\n          }\r\n        })\r\n      this.dialogVisible = false\r\n      this.load()\r\n    },\r\n    saveOrder(){\r\n      this.order={}\r\n      request.post(\"/api/dishorder/saveOrder\",this.order).then(res => {\r\n        console.log(res)\r\n        if (res.code == 0) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"提交成功\"\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n\r\n    handleEdit(good) {\r\n      this.form = JSON.parse(JSON.stringify(good))\r\n      this.dialogVisible = true\r\n    },\r\n\r\n    handleSizeChange(pageSize) {\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n\r\n    handleCurrentChange(pageNum) {\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>"]}]}