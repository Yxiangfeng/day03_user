em label=\"用户名\" >\r\n            <el-input v-model=\"form.name\" disabled style=\"margin-bottom: 20px\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"密码\">\r\n            <el-input v-model=\"form.password\" show-password style=\"margin-bottom: 20px\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"年龄\">\r\n            <el-input v-model=\"form.age\" style=\"margin-bottom: 20px\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"性别\">\r\n            <el-input v-model=\"form.gender\" style=\"margin-bottom: 20px\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"邮箱\">\r\n            <el-input v-model=\"form.email\" style=\"margin-bottom: 20px\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div style=\"text-align: center\">\r\n          <el-button type=\"primary\" @click=\"update\">保存</el-button>\r\n        </div>\r\n      </el-card>\r\n    </div>\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"indexInfo\",\r\n  data() {\r\n    return {\r\n      form: {}\r\n    }\r\n  },\r\n  created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.form = JSON.parse(str)\r\n  },\r\n  methods: {\r\n    update() {\r\n      request.put(\"/api/allUser/updateData\", this.form).then(res => {\r\n        console.log(res)\r\n        if (res.code == '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"更新成功\"\r\n          })\r\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form))\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":""}]}