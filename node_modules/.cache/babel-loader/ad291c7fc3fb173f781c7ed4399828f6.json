t>\r\n      <el-button type=\"primary\" style=\"margin-left:5px\" @click=\"load\">查询</el-button>\r\n    </div>\r\n    <el-table\r\n        :data=\"tableData\"\r\n        border\r\n        style=\"width: 100%\">\r\n      <el-table-column\r\n          sortable\r\n          prop=\"orderID\"\r\n          label=\"序号\" width=\"80px\">\r\n      </el-table-column>\r\n      <el-table-column\r\n          prop=\"dishFlag\"\r\n          label=\"编号\">\r\n      </el-table-column>\r\n      <el-table-column\r\n          prop=\"ordManagerName\"\r\n          label=\"点餐人\">\r\n      </el-table-column>\r\n      <el-table-column\r\n          prop=\"isServe\"\r\n          label=\"是否上菜\">\r\n      </el-table-column>\r\n      <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\">\r\n        <template #default=\"scope\">\r\n          <el-button type=\"primary\" size=\"mini\" @click=\"handleEdit(scope.row)\">详情</el-button>\r\n          <el-popconfirm\r\n              confirmButtonText='好的'\r\n              cancelButtonText='等等'\r\n              icon=\"el-icon-info\"\r\n              iconColor=\"red\"\r\n              title=\"确认上菜完毕吗\" @confirm=\"served(scope.row)\"\r\n          >\r\n            <template #reference>\r\n              <el-button size=\"mini\" type=\"danger\">上菜</el-button>\r\n            </template>\r\n          </el-popconfirm>\r\n        </template>\r\n\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"margin-bottom: 80px\"></div>\r\n    <div style=\"margin: 20px 0 0 500px\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[5, 10, 15]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"所有订单\" v-model=\"dialogVisible\" width=\"60%\" >\r\n      <el-table\r\n          :data=\"dishData\"\r\n          border\r\n          style=\"width: 100%;height: 400px\">\r\n        <el-table-column\r\n            prop=\"dishFlag\"\r\n            label=\"订单编号\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"dishName\"\r\n            label=\"菜名\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"dishPrice\"\r\n            label=\"单价\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"dishNum\"\r\n            label=\"数量\">\r\n        </el-table-column>\r\n      </el-table>\r\n      <div style=\"margin: 10px 0\">\r\n        <el-pagination\r\n            @size-change=\"handleSizeChange1\"\r\n            @current-change=\"handleCurrentChange1\"\r\n            :current-page=\"currentPage1\"\r\n            :page-sizes=\"[5, 10, 15]\"\r\n            :page-size=\"pageSize1\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"total1\">\r\n        </el-pagination>\r\n      </div>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">返回</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nexport default {\r\n  name:\"StaffOrder\",\r\n  components:{\r\n  },\r\n  data() {\r\n    return {\r\n      order:{},\r\n      dialogVisible:false,\r\n      currentPage:1,\r\n      pageSize:5,\r\n      total:0,\r\n      tableData: [],\r\n      search:'',\r\n      currentPage1:1,\r\n      pageSize1:5,\r\n      total1:0,\r\n      dishData:[]\r\n    }\r\n  },\r\n  created(){\r\n    this.load();\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get(\"/api/dishorder/findAllOrders\", {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n        }\r\n      }).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total=res.data.total\r\n        this.currentPage=res.data.current\r\n        this.pageSize=res.data.size\r\n      })\r\n    },\r\n\r\n    served(row){\r\n      this.order = JSON.parse(JSON.stringify(row))\r\n\r\n      request.post(\"/api/dishorder/serve\", this.order).then(res => {\r\n        console.log(res)\r\n        if (res.code == 0) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"上菜成功\"\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n      this.load()\r\n    },\r\n\r\n    handleEdit(row){\r\n      this.$nextTick(()=>{\r\n        this.order = JSON.parse(JSON.stringify(row))\r\n        request.post(\"/api/dishorder/saveOrderObj\", this.order)\r\n        this.dialogVisible = true\r\n        request.get(\"/api/dishorder/findDish\",{\r\n          params:{\r\n            pageNum1:this.currentPage1,\r\n            pageSize1:this.pageSize1,\r\n          }\r\n        }).then(res =>{\r\n          console.log(res)\r\n          this.dishData = res.data.records\r\n          this.total1=res.data.total\r\n          this.currentPage1=res.data.current\r\n          this.pageSize1=res.data.size\r\n        })\r\n      })\r\n    },\r\n\r\n    handleSizeChange(pageSize) {\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n\r\n    handleCurrentChange(pageNum) {\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    },\r\n\r\n    handleSizeChange1(pageSize) {\r\n      this.pageSize1 = pageSize\r\n      this.handleEdit(row)\r\n    },\r\n\r\n    handleCurrentChange1(pageNum) {\r\n      this.currentPage1 = pageNum\r\n      this.handleEdit(row)\r\n    }\r\n\r\n  }\r\n}\r\n</script>"],"sourceRoot":""}]}