-item>\r\n        <el-form-item label=\"邮箱\">\r\n          <el-input v-model=\"form.email\" style=\"margin-bottom: 20px\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图片\">\r\n          <el-upload ref=\"upload\" class=\"upload-demo\" action=\"http://localhost:8887/files/upload\" :on-success=\"filesUploadSuccess\">\r\n            <el-button type=\"primary\">点击上传</el-button>\r\n          </el-upload>\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n          <span class=\"dialog-footer\">\r\n            <el-button @click=\"cancel\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"update\">确 定</el-button>\r\n          </span>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Info\",\r\n  data() {\r\n    return {\r\n      form: {},\r\n      dialogVisible2:false,\r\n      userPhoto:''\r\n    }\r\n  },\r\n  created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.form = JSON.parse(str)\r\n    this.$store.commit(\"update\",this.form)\r\n    this.userPhoto = this.$store.state.userPhoto\r\n  },\r\n  methods: {\r\n    handleSelect(){\r\n      this.dialogVisible2 = true\r\n      this.$refs['upload'].clearFiles()\r\n\r\n    },\r\n    filesUploadSuccess(res){\r\n      console.log(res)\r\n      this.form.photo = res.data\r\n    },\r\n    cancel(){\r\n      let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n      this.form = JSON.parse(str)\r\n      this.dialogVisible2 = false\r\n    },\r\n    update() {\r\n      request.put(\"/api/allUser/updateData\", this.form).then(res => {\r\n        console.log(res)\r\n        if (res.code === 0) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"更新成功\"\r\n          })\r\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form))\r\n          let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n          this.form = JSON.parse(str)\r\n          this.$store.commit(\"update\",this.form)\r\n          this.userPhoto = this.$store.state.userPhoto\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n\r\n      this.dialogVisible2 = false\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container{\r\n  width:1250px;\r\n  height:1000px;\r\n}\r\n.box-card{\r\n  display:flex;\r\n  margin-top: 10px;\r\n  align-items: center;\r\n}\r\n.box-card > img{\r\n  margin-left: 50px;\r\n  width:240px;\r\n  height:240px;\r\n}\r\n.caption{\r\n  margin-left: 100px;\r\n  height:240px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: flex-start;\r\n  align-items: center;\r\n}\r\n.caption > h1,.caption > h2{\r\n  width:200px;\r\n  font-weight: bold;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":""}]}