+D;gCAA5C,KAAA,CAAA,IAAA,CAAK,QAAuC;;6BAA5C,KAAA,CAAA,IAAA,CAAK,QAAL,GAAa,M;sBAA+B;AAA7B,oBAAA,KAAiB,EAAjB;AAAA,+BAAA;AAAA;AAA6B,mBAA/D,E,IAAA,E,CAAA,E,cAAA,CAA+D,CAA/D;AAAA,iB,CADsB;;AAAA,eAAxB,CAEe,EACf,YAAA,CAEe,uBAFf,EAEe;AAFD,gBAAA,KAAK,EAAC;AAEL,eAFf,EAAwB;kCACtB;AAAA,yBAAgE,CAAhE,YAAA,CAAgE,mBAAhE,EAAgE;gCAA7C,KAAA,CAAA,IAAA,CAAK,SAAwC;;6BAA7C,KAAA,CAAA,IAAA,CAAK,SAAL,GAAc,M;sBAA+B;AAA7B,oBAAA,KAAiB,EAAjB;AAAA,+BAAA;AAAA;AAA6B,mBAAhE,E,IAAA,E,CAAA,E,cAAA,CAAgE,CAAhE;AAAA,iB,CADsB;;AAAA,eAAxB,CADe,EAIf,YAAA,CAEe,uBAFf,EAEe;AAFD,gBAAA,KAAK,EAAC;AAEL,eAFf,EAAwB;kCACtB;AAAA,yBAA4F,CAA5F,YAAA,CAA4F,mBAA5F,EAA4F;gCAAzE,KAAA,CAAA,IAAA,CAAK,QAAoE;;6BAAzE,KAAA,CAAA,IAAA,CAAK,QAAL,GAAa,M;sBAA4D;AAA1D,oBAAA,KAAiB,EAAjB;AAAA,+BAAA;AAAA,qBAA0D;AAAxC,oBAAA,WAAW,EAAC;AAA4B,mBAA5F,E,IAAA,E,CAAA,E,cAAA,CAA4F,CAA5F;AAAA,iB,CADsB;;AAAA,eAAxB,CAJe,EAOf,YAAA,CAEe,uBAFf,EAEe;AAFD,gBAAA,KAAK,EAAC;AAEL,eAFf,EAAwB;kCACtB;AAAA,yBAAuF,CAAvF,YAAA,CAAuF,mBAAvF,EAAuF;gCAApE,KAAA,CAAA,IAAA,CAAK,UAA+D;;6BAApE,KAAA,CAAA,IAAA,CAAK,UAAL,GAAe,M;sBAAqD;AAAnD,oBAAA,KAAiB,EAAjB;AAAA,+BAAA;AAAA,qBAAmD;AAAjC,oBAAA,WAAW,EAAC;AAAqB,mBAAvF,E,IAAA,E,CAAA,E,cAAA,CAAuF,CAAvF;AAAA,iB,CADsB;;AAAA,eAAxB,CAPe,EAUf,YAAA,CAIe,uBAJf,EAIe;AAJD,gBAAA,KAAK,EAAC;AAIL,eAJf,EAAwB;kCACtB;AAAA,yBAEY,CAFZ,YAAA,CAEY,oBAFZ,EAEY;AAFD,oBAAA,GAAG,EAAC,QAEH;AAFY,oBAAA,KAAK,EAAC,aAElB;AAFgC,oBAAA,MAAM,EAAC,oCAEvC;AAF6E,kCAAY,QAAA,CAAA;AAEzF,mBAFZ,E;sCACE;AAAA,6BAA0C,CAA1C,YAAA,CAA0C,oBAA1C,EAA0C;AAA/B,wBAAA,IAAI,EAAC;AAA0B,uBAA1C,EAAyB;0CAAC;AAAA,iCAAI,C,UAAA,CAAJ;AAAA,yB,CAAD;;AAAA,uBAAzB,CAA0C,CAA1C;AAAA,qB;;mBADF,E,CAAA,E,cAAA,CAEY,CAFZ;AAAA,iB,CADsB;;AAAA,eAAxB,CAVe,CAFf;AAAA,a;;WADF,E,CAAA,E,SAAA,CAkBU,CAlBV;AAAA,S;;OADF,E,CAAA,E,cAAA,CA4BI,CA/GN,CA+GM,CA/GN;AAAA,K,CADuB;;AAAA,GAAzB,C","sourcesContent":["<template>\r\n  <el-card class=\"box-card\" >\r\n    <div class=\"home\" style=\"padding: 10px\">\r\n      <div style=\"margin: 10px 0\">\r\n        <el-input v-model=\"search\" placeholder=\"please input name\" style=\"width:20%\" clearable></el-input>\r\n        <el-button type=\"primary\" style=\"margin-left:5px\" @click=\"load\">查询</el-button>\r\n        <el-button type=\"primary\" @click=\"Add\">新增</el-button>\r\n      </div>\r\n      <el-table\r\n          :header-cell-style=\"{background:'#F0F8FF',color:'#303133'}\"\r\n          :data=\"tableData\" stripe\r\n          style=\"width: 100%\">\r\n        <el-table-column\r\n            sortable\r\n            align=\"center\"\r\n            prop=\"dishID\"\r\n            label=\"编号\" width=\"80px\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            align=\"center\"\r\n            prop=\"dishName\"\r\n            label=\"菜名\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            align=\"center\"\r\n            prop=\"dishPrice\"\r\n            label=\"价格\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            align=\"center\"\r\n            prop=\"dishType\"\r\n            label=\"类型\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            align=\"center\"\r\n            prop=\"dishStatus\"\r\n            label=\"有无\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            align=\"center\"\r\n            label=\"图片\">\r\n          <template #default=\"scope\">\r\n            <el-image\r\n                style=\"width: 120px; height: 120px\"\r\n                :src=\"scope.row.dishPicPath\"\r\n                :preview-src-list=\"[scope.row.dishPicPath]\">\r\n            </el-image>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n            align=\"center\"\r\n            fixed=\"right\"\r\n            label=\"操作\">\r\n          <template #default=\"scope\">\r\n            <el-button type=\"primary\" icon=\"el-icon-edit\" circle size=\"normal\" @click=\"handleEdit(scope.row)\"></el-button>\r\n            <el-popconfirm\r\n                confirmButtonText='好的'\r\n                cancelButtonText='不用了'\r\n                icon=\"el-icon-info\"\r\n                iconColor=\"red\"\r\n                title=\"这道菜确定删除吗？\" @confirm=\"handleDelete(scope.row.dishID)\"\r\n            >\r\n              <template #reference>\r\n                <el-button type=\"danger\" icon=\"el-icon-delete\" circle size=\"normal\"></el-button>\r\n              </template>\r\n            </el-popconfirm>\r\n          </template>\r\n\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <div style=\"margin-bottom: 20px\"></div>\r\n      <div style=\"margin: 10px 0\">\r\n        <el-pagination\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"currentPage\"\r\n            :page-sizes=\"[3, 6, 9]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"total\"\r\n        >\r\n        </el-pagination>\r\n      </div>\r\n\r\n      <el-dialog title=\"增加菜品\" v-model=\"dialogVisible\" width=\"30%\">\r\n        <el-form :model=\"form\" label-width=\"120px\">\r\n          <el-form-item label=\"菜名\">\r\n            <el-input v-model=\"form.dishName\" style=\"width:80%\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"价格\">\r\n            <el-input v-model=\"form.dishPrice\" style=\"width:80%\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"类型\">\r\n            <el-input v-model=\"form.dishType\" style=\"width:80%\" placeholder=\"1:西餐 2:中餐 3:甜点\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"有无\">\r\n            <el-input v-model=\"form.dishStatus\" style=\"width:80%\" placeholder=\"1:有 2:无\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"图片\">\r\n            <el-upload ref=\"upload\" class=\"upload-demo\" action=\"http://localhost:8888/files/upload\" :on-success=\"filesUploadSuccess\">\r\n              <el-button type=\"primary\">点击上传</el-button>\r\n            </el-upload>\r\n          </el-form-item>\r\n        </el-form>\r\n        <template #footer>\r\n          <span class=\"dialog-footer\">\r\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"saveData\">确 定</el-button>\r\n          </span>\r\n        </template>\r\n      </el-dialog>\r\n\r\n    </div>\r\n  </el-card>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\n\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name:\"dish\",\r\n  components:{\r\n  },\r\n  data() {\r\n    return {\r\n      form:{},\r\n      dialogVisible:false,\r\n      search:'',\r\n      currentPage:1,\r\n      pageSize:3,\r\n      total:0,\r\n      tableData: []\r\n    }\r\n  },\r\n  created(){\r\n    this.load();\r\n  },\r\n  methods:{\r\n    filesUploadSuccess(res){\r\n      console.log(res)\r\n      this.form.dishPicPath = res.data\r\n    },\r\n    load(){\r\n      request.get(\"/api/dish/findPage\",{\r\n        params:{\r\n          pageNum:this.currentPage,\r\n          pageSize:this.pageSize,\r\n          search:this.search\r\n        }\r\n      }).then(res =>{\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total=res.data.total\r\n        this.currentPage=res.data.current\r\n        this.pageSize=res.data.size\r\n      })\r\n    },\r\n\r\n    Add(){\r\n      this.dialogVisible = true\r\n      this.form={}\r\n      this.$refs['upload'].clearFiles()\r\n    },\r\n\r\n    saveData(){\r\n      if(this.form.dishID){\r\n        request.put(\"/api/dish/updateData\",this.form).then(res =>{\r\n          console.log(res)\r\n          if(res.code == 0){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"修改成功\"\r\n            })}else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n        })\r\n      }else {\r\n        request.post(\"/api/dish/saveData\", this.form).then(res => {\r\n          console.log(res)\r\n          if(res.code == 0){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"添加成功\"\r\n            })}else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.msg\r\n            })\r\n          }\r\n        })\r\n      }\r\n      this.dialogVisible = false\r\n      this.load()\r\n    },\r\n\r\n    handleEdit(row){\r\n     //\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogVisible = true\r\n      this.$nextTick(()=>{\r\n        this.$refs['upload'].clearFiles()\r\n      })\r\n\r\n    },\r\n\r\n    handleDelete(dishID){\r\n      console.log(dishID)\r\n      request.delete(\"/api/dish/\"+dishID).then(res => {\r\n        console.log(res)\r\n        if(res.code == 0){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"删除成功\"\r\n          })}else{\r\n          this.$message({\r\n            type:\"error\",\r\n            message:res.msg\r\n          })\r\n        }\r\n      })\r\n      this.load()\r\n    },\r\n\r\n    handleSizeChange(pageSize){\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n\r\n    handleCurrentChange(pageNum){\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    }\r\n\r\n\r\n  }\r\n}\r\n</script>"],"sourceRoot":""}]}