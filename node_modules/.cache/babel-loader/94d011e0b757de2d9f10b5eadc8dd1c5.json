t.commentPersonName}}</span>\r\n    <hr/>\r\n\r\n    <button class=\"cancle\" v-if=\"user.role === 1\">删除帖子</button>\r\n    <a href=\"#\"><i class=\"el-icon-caret-top\"></i></a>\r\n\r\n    <div class=\"headerBox\">\r\n      <img :src=\"comment.commentPersonPic\"/>\r\n      <div class=\"info\">\r\n        <div>{{comment.commentPersonName}}</div>\r\n        <div>{{comment.commentTime}}</div>\r\n      </div>\r\n      <h2>{{comment.commentTitle}}</h2>\r\n    </div>\r\n\r\n    <el-divider></el-divider>\r\n\r\n    <div class=\"box\">\r\n      <div class=\"main\">\r\n        <div style=\"margin-left: 36px;font-size: 18px;font-weight: bold;\">{{comment.commentDes}}</div>\r\n        <div v-html=\"comment.commentContent\"></div>\r\n      </div>\r\n      <div class=\"pinglun\">\r\n        <input v-model=\"search\" placeholder=\"发表评论\" type=\"text\"/>\r\n        <el-button style=\"margin-left: 10px;\" type=\"primary\" round disabled>提交</el-button>\r\n      </div>\r\n    </div>\r\n    <div>\r\n\r\n    </div>\r\n\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"tiezi\",\r\n  data() {\r\n    return {\r\n      comment: {},\r\n      dialogVisible2:false,\r\n      user:{},\r\n      search:'',\r\n    }\r\n  },\r\n  created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(str)\r\n\r\n    this.load()\r\n  },\r\n  methods: {\r\n     load(){\r\n       request.get(\"/api/comment/ctInfo\").then(res =>{\r\n         console.log(res)\r\n         this.comment = res.data\r\n       })\r\n     },\r\n    returnLast(){\r\n      this.$router.push('/commentList')\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .headerBox{\r\n    display: flex;\r\n  }\r\n\r\n  .headerBox img{\r\n    width: 80px;\r\n    height: 80px;\r\n    border-radius:50%;\r\n    margin-left: 15px;\r\n  }\r\n\r\n  .headerBox .info{\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .headerBox > h2{\r\n    margin-top: 18px;\r\n    margin-left: 240px;\r\n\r\n  }\r\n\r\n  .info > div{\r\n    margin-left: 20px;\r\n    margin-top: 15px;\r\n    width: 150px;\r\n  }\r\n\r\n  .info :first-child{\r\n    font-size: 18px;\r\n    font-weight: bold;\r\n  }\r\n\r\n  .info :last-child{\r\n    color:gray;\r\n    font-size: 13px;\r\n  }\r\n\r\n  a{\r\n    width:40px;\r\n    height: 40px;\r\n    position: fixed;border-radius: 50%;\r\n    bottom:80px;\r\n    right:45px;\r\n    background-color: #F0F8FF;\r\n    border:1px solid #C6E2FF;\r\n  }\r\n\r\n  a i{\r\n    position: fixed;\r\n    bottom: 93px;\r\n    right: 56px;\r\n  }\r\n\r\n  a:hover{\r\n    background: linear-gradient(200deg,#f3e7e9,#e3eeff);\r\n    transition: background-color 0.5s ease;\r\n  }\r\n\r\n  .cancle{\r\n    width: 63px;\r\n    height: 25px;\r\n    background-color: #F0F8FF;\r\n    border:1px solid #C6E2FF;\r\n    border-radius: 3px;\r\n    position: absolute;\r\n    top:77px;\r\n    right:20px;\r\n  }\r\n\r\n  .cancle:hover{\r\n    background: linear-gradient(200deg,#f3e7e9,#e3eeff);\r\n    transition: background-color 0.5s ease;\r\n  }\r\n\r\n  .main{\r\n    width:680px;\r\n    height: 400px;\r\n    margin-left: 20px;\r\n    border-radius: 10px;\r\n    border: 1px solid whitesmoke;\r\n    border-bottom: transparent;\r\n  }\r\n\r\n  .pinglun{\r\n    width: 480px;\r\n    margin-left: 10px;\r\n    border-radius: 10px;\r\n    border: 1px solid whitesmoke;\r\n    border-bottom: transparent;\r\n  }\r\n\r\n  .box{\r\n    width: 1200px;\r\n    display: flex;\r\n  }\r\n\r\n  input{\r\n    width:350px;\r\n    line-height: 25px;\r\n    font-size: 20px;\r\n    margin-left: 45px;\r\n    border:1px solid blue;\r\n  }\r\n\r\n\r\n</style>\r\n"],"sourceRoot":""}]}