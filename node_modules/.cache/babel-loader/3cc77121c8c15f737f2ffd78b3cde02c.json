"width:200px;min-height:100vh\" router class=\"el-menu-vertical-demo\">\r\n          <el-submenu index=\"1\">\r\n            <template #title>\r\n              <i class=\"el-icon-location\"></i><span>菜系</span>\r\n            </template>\r\n            <el-menu-item><a href=\"#\" class=\"btn btn-default\" role=\"button\" @click=\"zc\">中餐</a></el-menu-item>\r\n            <el-menu-item><a href=\"#\" class=\"btn btn-default\" role=\"button\" @click=\"soup\">汤</a></el-menu-item>\r\n            <el-menu-item><a href=\"#\" class=\"btn btn-default\" role=\"button\" @click=\"xc\">西餐</a></el-menu-item>\r\n            <el-menu-item><a href=\"#\" class=\"btn btn-default\" role=\"button\" @click=\"dessert\">点心</a></el-menu-item>\r\n            <el-menu-item><a href=\"#\" class=\"btn btn-default\" role=\"button\" @click=\"drink\">饮料</a></el-menu-item>\r\n          </el-submenu>\r\n\r\n        </el-menu>\r\n\r\n      </div>\r\n      <div class=\"head1\" style=\"position: relative;top:50px\">\r\n        <div style=\"padding: 50px;\" v-for=\"(good, index) in tableData\" :key=\"good.dishID\">\r\n          <img :src=\"good.dishPicPath\" style=\"margin-bottom: 30px\" >\r\n          <div class=\"caption\">\r\n            <span style=\"margin-left:80px;font-weight: bold\" hidden>{{good.dishID}}</span>\r\n            <span style=\"margin-left:80px;font-weight: bold\">{{good.dishName}}</span>\r\n            <span style=\"margin-left: 20px;margin-bottom: 100px\">￥{{good.dishPrice}}</span>\r\n            <div style=\"margin-bottom: 20px\"></div>\r\n            <p style=\"margin-left: 90px;\"><el-button type=\"default\" size=\"normal\" @click=\"handleEdit(good)\">选择</el-button></p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div>\r\n      <el-pagination style=\"position: absolute;top:650px;left:500px\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[4]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n    <div style=\"position: absolute;top:645px;left:1000px\">\r\n      <el-button type=\"default\" size=\"normal\" @click=\"saveOrder\">提交订单</el-button>\r\n    </div>\r\n\r\n    <hr>\r\n\r\n    <el-dialog title=\"点菜\" v-model=\"dialogVisible\" width=\"30%\">\r\n      <el-form :model=\"form\" label-width=\"120px\">\r\n        <el-form-item label=\"菜名\">\r\n          <el-input v-model=\"form.dishName\" style=\"width:80%\" readonly></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"价格\">\r\n          <el-input v-model=\"form.dishPrice\" style=\"width:80%\" readonly></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"数量\">\r\n          <el-input v-model=\"form.dishNum\" style=\"width:80%\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"dishData\">确 定</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n*{\r\n  /*background-color:#e5e5e5;*/\r\n}\r\n    .head1{\r\n      display: flex;\r\n      max-height:700px;\r\n    }\r\n    img{\r\n      width:200px;\r\n      height:200px;\r\n    }\r\n</style>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport IndexAside from \"@/components/IndexAside\";\r\nexport default {\r\n  name: \"IndexOrder\",\r\n  components: {IndexAside},\r\n  data() {\r\n    return {\r\n      form: {},\r\n      dialogVisible: false,\r\n      search: '',\r\n      currentPage: 1,\r\n      pageSize: 4,\r\n      total: 0,\r\n      tableData: [],\r\n      user:{},\r\n      order:{}\r\n\r\n    }\r\n  },\r\n  created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(str)\r\n    this.load();\r\n  },\r\n  methods: {\r\n    Flag(){\r\n      //alert(\"123\")\r\n      request.post(\"/api/dishorder/saveID\", this.user) //传递用户ID\r\n      request.post(\"/api/dishorder/saveFlag\").then(res => {//生成唯一的订单号，并加到点菜表里面\r\n        console.log(res)\r\n        if (res.code == 0) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"请开始点餐\"\r\n          })\r\n        }\r\n      })\r\n\r\n    },\r\n    zc(){request.get(\"/api/dish/findDish\", {\r\n      params: {\r\n        pageNum: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: '0'\r\n      }\r\n      }).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n        this.currentPage = res.data.current\r\n        this.pageSize = res.data.size\r\n      })\r\n    },\r\n    xc(){request.get(\"/api/dish/findDish\", {\r\n      params: {\r\n        pageNum: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: '1'\r\n      }\r\n    }).then(res => {\r\n      console.log(res)\r\n      this.tableData = res.data.records\r\n      this.total = res.data.total\r\n      this.currentPage = res.data.current\r\n      this.pageSize = res.data.size\r\n    })\r\n    },\r\n    soup(){request.get(\"/api/dish/findDish\", {\r\n      params: {\r\n        pageNum: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: '2'\r\n      }\r\n    }).then(res => {\r\n      console.log(res)\r\n      this.tableData = res.data.records\r\n      this.total = res.data.total\r\n      this.currentPage = res.data.current\r\n      this.pageSize = res.data.size\r\n    })\r\n    },\r\n    dessert(){request.get(\"/api/dish/findDish\", {\r\n      params: {\r\n        pageNum: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: '3'\r\n      }\r\n    }).then(res => {\r\n      console.log(res)\r\n      this.tableData = res.data.records\r\n      this.total = res.data.total\r\n      this.currentPage = res.data.current\r\n      this.pageSize = res.data.size\r\n    })\r\n    },\r\n    drink(){request.get(\"/api/dish/findDish\", {\r\n      params: {\r\n        pageNum: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        search: '4'\r\n      }\r\n    }).then(res => {\r\n      console.log(res)\r\n      this.tableData = res.data.records\r\n      this.total = res.data.total\r\n      this.currentPage = res.data.current\r\n      this.pageSize = res.data.size\r\n    })\r\n    },\r\n    load() {\r\n      request.get(\"/api/dish/findPage\", {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n        }\r\n      }).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n        this.currentPage = res.data.current\r\n        this.pageSize = res.data.size\r\n      })\r\n    },\r\n\r\n    dishData() {\r\n        request.post(\"/api/dishorder/saveData\", this.form).then(res => {\r\n          console.log(res)\r\n          if (res.code == 0) {\r\n            this.$message({\r\n              type: \"success\",\r\n              message: \"添加成功\"\r\n            })\r\n          } else {\r\n            this.$message({\r\n              type: \"error\",\r\n              message: res.msg\r\n            })\r\n          }\r\n        })\r\n      this.dialogVisible = false\r\n      this.load()\r\n    },\r\n    saveOrder(){\r\n      this.order={}\r\n      request.post(\"/api/dishorder/saveOrder\",this.order).then(res => {\r\n        console.log(res)\r\n        if (res.code == 0) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"提交成功\"\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n\r\n    handleEdit(good) {\r\n      this.form = JSON.parse(JSON.stringify(good))\r\n      this.dialogVisible = true\r\n    },\r\n\r\n    handleSizeChange(pageSize) {\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n\r\n    handleCurrentChange(pageNum) {\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>"],"sourceRoot":""}]}